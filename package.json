{
  "name": "language-less",
  "displayName": "Language Less",
  "description": "Less Language advanced syntax highlight.",
  "version": "0.0.1",
  "publisher": "MaxwellBento",
  "author": {
    "name": "Maxwell Bento"
  },
  "engines": {
    "vscode": "^1.82.0"
  },
  "repository": {
    "url": "https://github.com/maxwell-ilai/language-less"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "less",
        "aliases": [
          "Less",
          "less"
        ],
        "extensions": [
          ".less"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "less",
        "scopeName": "source.css.less",
        "path": "./syntaxes/less.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npx js-yaml syntaxes/less.tmLanguage.yaml > syntaxes/less.tmLanguage.json"
  },
  "devDependencies": {
    "js-yaml": "^4.1.0"
  }
}
